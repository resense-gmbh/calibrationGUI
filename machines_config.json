  "scripts": {
    "Raw_Step.py": {
      "name": "Raw Step Analysis",
      "description": "Basic analysis with raw data processing and plots",
      "category": "basic",
      "outputs": ["plots", "data"],
      "requirements": ["numpy", "matplotlib", "pandas"]
    },
    "Analysis.py": {
      "name": "Full Analysis Suite",
      "description": "Complete analysis with calibration matrix calculation",
      "category": "advanced",
      "outputs": ["plots", "matrix", "report"],
      "requirements": ["numpy", "matplotlib", "pandas", "scipy"]
    },
    "calculateMatrix.py": {
      "name": "Matrix Calculation",
      "description": "Calculate calibration matrix from measurement data",
      "category": "calculation",
      "outputs": ["matrix", "coefficients"],
      "requirements": ["numpy", "scipy"]
    },
    "DataVisualization.py": {
      "name": "Data Visualization",
      "description": "Generate comprehensive plots and visualizations",
      "category": "visualization",
      "outputs": ["plots", "charts"],
      "requirements": ["matplotlib", "seaborn", "pandas"]
    }
  },
